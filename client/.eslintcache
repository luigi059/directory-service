age if user is logged in\n  const checkLoggedIn = async () => {\n    let token = localStorage.getItem(\"auth-token\");\n    if (token === null) {\n      localStorage.setItem(\"auth-token\", \"\");\n      token = \"\";\n    }\n    const tokenRes = await axios.post(\n      \"http://localhost:5000/users/tokenIsValid\",\n      null,\n      { headers: { \"x-auth-token\": token } }\n    );\n    if (tokenRes.data) {\n      const userRes = await axios.get(\"http://localhost:5000/users/\", {\n        headers: { \"x-auth-token\": token },\n      });\n      setUserData({\n        token,\n        user: userRes.data,\n      });\n    }\n  };\n\n  useEffect(() => {\n    checkLoggedIn();\n  }, []);\n\n  return (\n    <Router>\n      {/* all components now have access to userData thanks to useContext */}\n      <UserContext.Provider value={{userData,setUserData}}>\n      <div>\n        <Header/>\n        <Switch>\n{/*           <Route exact path=\"/\" component={Home} /> */}\n          <Route path=\"/favourites\" component={Favourite} />\n          <Route path=\"/login\" component={Login} />\n          <Route path=\"/register\" component={Register} />\n        </Switch>\n        <Footer/>\n      </div>\n      </UserContext.Provider>\n    </Router>\n  );    \n}\n\nexport default App;","C:\\Users\\Acer\\Documents\\Programming\\PortfolioProjects\\DirectoryService\\client\\src\\components\\searchbar\\Searchbar.js",[],"C:\\Users\\Acer\\Documents\\Programming\\PortfolioProjects\\DirectoryService\\client\\src\\components\\api\\api.js",[],["70","71"],"C:\\Users\\Acer\\Documents\\Programming\\PortfolioProjects\\DirectoryService\\client\\src\\components\\gamelist\\GameList.js",[],"C:\\Users\\Acer\\Documents\\Programming\\PortfolioProjects\\DirectoryService\\client\\src\\components\\gamelist\\GameItem.js",["72"],"import React,{useEffect, useState, useRef, useContext} from \"react\";\r\nimport {useHistory} from \"react-router-dom\"; \r\nimport axios from \"axios\";\r\nimport UserContext from \"